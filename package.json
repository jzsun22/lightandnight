    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "unplugin-vue-components": "^0.21.1",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "vant": "^4.0.0-beta.0",
    "vconsole": "^3.14.6",
    "vercel": "^19.1.2",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
