    "babel-plugin-import": "^1.13.5",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "lint-staged": "^13.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "ie 11"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged && node tools/updateTime.js && git add src/data/app/time.json"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "git add"
    ],
    "src/data/*/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "description": "北极光工作室手机游戏《光与夜之恋》相关小工具集合，包含各类计算器与抽卡模拟器。",
  "main": "babel.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jzsun22/lightandnight.git"
